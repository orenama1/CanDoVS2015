<Window x:Class="KabeDon.Wpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KabeDon.Wpf"
        mc:Ignorable="d"
        Title="クラウディア(仮)" SizeToContent="WidthAndHeight">
    <Grid>
        <MediaElement x:Name="ExcellentSound" Source="Media/taiko03.mp3" LoadedBehavior="Manual" UnloadedBehavior="Stop" />
        <MediaElement x:Name="GoodSound" Source="Media/drum02.mp3" LoadedBehavior="Manual" UnloadedBehavior="Stop" />
        <MediaElement x:Name="EasterEggSound" Source="Media/bell03.mp3" LoadedBehavior="Manual" UnloadedBehavior="Stop" />
        <MediaElement x:Name="HatSound" Source="Media/voice_25hi.mp3" LoadedBehavior="Manual" UnloadedBehavior="Stop" />
        <MediaElement x:Name="FaceSound" Source="Media/37ah.wav" LoadedBehavior="Manual" UnloadedBehavior="Stop" />
        <Image x:Name="CloudiaImage" Source="Images/クラウディアdefault.png" Width="540" Height="960" />
        <!--<MediaElement x:Name="Sound1" Source="Sound/bell03.mp3" LoadedBehavior="Manual" UnloadedBehavior="Stop" />-->
    </Grid>
</Window>
